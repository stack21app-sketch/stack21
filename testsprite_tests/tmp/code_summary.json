  "authentication": {
    "type": "OAuth",
    "providers": ["Google", "GitHub"],
    "session_management": "NextAuth.js",
    "protected_routes": ["/dashboard", "/agents", "/analytics", "/smart-dashboard"]
  },
  
  "database": {
    "type": "PostgreSQL",
    "orm": "Prisma",
    "models": ["User", "Workspace", "Project", "Module", "Workflow", "RunLog", "Billing", "Analytics"]
  },
  
  "external_services": [
    "OpenAI API",
    "Stripe",
    "Supabase",
    "SendGrid",
    "Google OAuth",
    "GitHub OAuth"
  ],
  
  "port": 3000,
  "development_commands": {
    "start": "npm run dev",
    "build": "npm run build",
    "test": "npm run test"
  }
}