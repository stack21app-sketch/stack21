                "required": true,
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "event": {"type": "string"},
                        "data": {"type": "object"}
                      }
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Evento trackeado exitosamente",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
