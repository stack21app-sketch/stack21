{
  "project_name": "Stack21 - SaaS Platform with AI Agents",
  "project_type": "fullstack",
  "description": "Una plataforma SaaS multi-tenant completa construida con Next.js 14, TypeScript, Prisma, Stripe, NextAuth y OpenAI. Incluye sistema de agentes de IA especializados, facturación, autenticación, dashboard inteligente y automatización de workflows.",
  
  "tech_stack": [
    "Next.js 14",
    "React 18", 
    "TypeScript",
    "Tailwind CSS",
    "Radix UI",
    "Prisma ORM",
    "PostgreSQL",
    "NextAuth.js",
    "Stripe",
    "OpenAI API",
    "Supabase",
    "Jest",
    "Testing Library",
    "Framer Motion",
    "Lucide React",
    "Zod",
    "i18next"
  ],
  
  "features": [
    {
      "name": "AI Agents API",
      "description": "Sistema completo de agentes de IA especializados que pueden ejecutar tareas complejas como reservas de restaurantes, marketing automatizado y análisis de datos empresariales",
      "files": [
        "src/lib/ai-agents.ts",
        "src/app/api/agents/route.ts",
        "src/app/api/agents/executions/route.ts",
        "src/app/api/agents/executions/[id]/route.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "AI Agents API",
          "version": "1.0.0",
          "description": "API para gestión y ejecución de agentes de IA especializados"
        },
        "paths": {
          "/api/agents": {
            "get": {
              "summary": "Obtener agentes disponibles",
              "responses": {
                "200": {
                  "description": "Lista de agentes disponibles",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"},
                          "data": {
                            "type": "object",
                            "properties": {
                              "agents": {"type": "array"},
                              "total": {"type": "number"}
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "post": {
              "summary": "Ejecutar un agente",
              "requestBody": {
                "required": true,
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "agentId": {"type": "string"},
                        "input": {"type": "string"},
                        "context": {"type": "object"}
                      },
                      "required": ["agentId", "input"]
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Agente ejecutado exitosamente",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"},
                          "data": {
                            "type": "object",
                            "properties": {
                              "execution": {"type": "object"},
                              "agent": {"type": "object"}
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Authentication API",
      "description": "Sistema de autenticación con OAuth (Google, GitHub) y NextAuth.js",
      "files": [
        "src/lib/auth.ts",
        "src/app/api/auth/[...nextauth]/route.ts",
        "src/middleware.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "Authentication API",
          "version": "1.0.0",
          "description": "API para autenticación y autorización de usuarios"
        },
        "paths": {
          "/api/auth/signin": {
            "post": {
              "summary": "Iniciar sesión",
              "requestBody": {
                "required": true,
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "provider": {"type": "string", "enum": ["google", "github"]},
                        "code": {"type": "string"}
                      }
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Login exitoso",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"},
                          "user": {"type": "object"},
                          "session": {"type": "object"}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Billing API",
      "description": "Sistema completo de facturación con Stripe, planes de suscripción y gestión de pagos",
      "files": [
        "src/app/api/billing/route.ts",
        "src/lib/stripe.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "Billing API",
          "version": "1.0.0",
          "description": "API para gestión de facturación y suscripciones"
        },
        "paths": {
          "/api/billing": {
            "get": {
              "summary": "Obtener planes de facturación",
              "responses": {
                "200": {
                  "description": "Lista de planes disponibles",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "string"},
                            "name": {"type": "string"},
                            "price": {"type": "number"},
                            "features": {"type": "array"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "post": {
              "summary": "Crear suscripción",
              "requestBody": {
                "required": true,
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "planId": {"type": "string"},
                        "paymentMethodId": {"type": "string"}
                      }
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Suscripción creada exitosamente",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"},
                          "subscription": {"type": "object"}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Workspace API",
